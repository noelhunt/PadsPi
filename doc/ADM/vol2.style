func book(b) {
	# authorlist
			if(authors[b]) {
				printfull(b);
				printf "\n";
			}
	# italic title.
			if(title[b])
				printf "\\f2\\&%s\\f1\\&.\n", title[b];
	# Vol. #.
			if (volstr[b])
				printf "%s.\n", volstr[b] ;
			else if (volume[b])
				printf "Vol. %s.\n", volume[b] ;
	# publisher, location
			printf "%s", issuer[b] ;
			punc = ",\n";
			if (city[b]) printf ", %s", city[b] ;
	# date
			printf punc date[b] ;
}

func editedbook(b) {

